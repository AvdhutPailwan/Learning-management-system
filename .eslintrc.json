{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "backtick"],
        "semi": ["error", "always"]
    }
}
